  <DataSets>
    <DataSet Name="Dummybank_DataSet1">
      <Query>
        <DataSourceName>DummyBank_DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Loan_Purpose_Category">
            <Value>=Parameters!Loan_Purpose_Category.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  DimBank.AlternateBranchID
  ,DimBorrower.AlternateBorrowerID
  ,DimBorrower.BorrowerName
  ,DimBorrower.BorrowerCity
  ,DimLoanDetails.ApplicationType
  ,DimLoanPurpose.LoanPurpose
  ,DimDisbursementMethod.DisbursementMethod
  ,FactLoan.TermInMonths
  ,FactLoan.IssueDateKey
  ,FactLoan.LoanAmount
  ,FactLoan.InterestRate
  ,FactLoan.Installment
  ,FactLoan.MInterestRate
  ,FactLoan.TotalPayment
  ,FactLoan.TotalInterest
  ,DimBorrower.BorrowerStreet
  ,DimBorrower.BorrowerState
  ,DimLoanDetails.[Year]
FROM
  DimBorrower
  INNER JOIN DimLoanPurpose
    ON DimBorrower.LoanPurposeKey = DimLoanPurpose.LoanPurposeSK
  INNER JOIN FactLoan
    ON DimBorrower.BorrowerSK = FactLoan.BorrowerKey
  INNER JOIN DimBank
    ON FactLoan.BranchKey = DimBank.BranchSK
  INNER JOIN DimLoanDetails
    ON FactLoan.LoanDetailsKey = DimLoanDetails.LoanDetailsSK
  INNER JOIN DimDisbursementMethod
    ON FactLoan.DisbursementKey = DimDisbursementMethod.DisbursementSK
WHERE DimLoanPurpose.LoanPurpose = @Loan_Purpose_Category</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
